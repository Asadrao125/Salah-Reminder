<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:left="2dp"
        android:right="2dp"
        android:gravity="center_horizontal">


        <shape android:shape="rectangle">
            <solid android:color="#dfe1de"/>
            <corners android:radius="100dp"/>
        </shape>
    </item>



    <item
        android:gravity="center_horizontal"
        android:bottom="6dp">
        <shape android:shape="rectangle">

            <corners android:radius="80dp"/>
            <solid android:color="#3f8ba2"/>
            <gradient
                android:startColor="#297790"
                android:endColor="#52a3b8"
                android:angle="270"/>
        </shape>
    </item>

</layer-list>
